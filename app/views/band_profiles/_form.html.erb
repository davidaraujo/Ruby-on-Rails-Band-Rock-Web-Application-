<%= simple_form_for(@band_profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :info %>
    <%= f.input :facebook %>
    <%= f.input :twitter %>
    <%= f.input :soundcloud %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
